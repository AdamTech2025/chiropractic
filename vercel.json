{
  "version": 2,
  "buildCommand": "npm run build",
  "outputDirectory": "dist",
  "routes": [
    {
      "src": "/api/upload",
      "dest": "/api/upload.js"
    },
    {
      "src": "/api/eligibility",
      "dest": "/api/eligibility.js"
    }
  ],
  "functions": {
    "api/upload.js": {
      "maxDuration": 30
    },
    "api/eligibility.js": {
      "maxDuration": 30
    }
  },
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ]
} 